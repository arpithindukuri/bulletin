    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "cors": "^2.8.5",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "firebase-functions-test": "^0.2.3",
    "mocha": "^9.2.2",
    "typescript": "^3.8.0"
  },
  "private": true
}
